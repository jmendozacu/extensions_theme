 	<script type="text/javascript">
 		function sendRequest() {
 			jQuery('.ajax-loader').show();	
 			jQuery('.btn-contactus').attr('disabled'); 
			jQuery.ajax({
		        url: '<?php echo Mage::getUrl('contacts/index/postAjax');?>',
		        type: 'post',
		        dataType: 'json',
		        data: jQuery('#contactus input, #contactus textarea').serialize(),
		        success: function(response) {		 
		        	jQuery('.ajax-loader').hide();	  
		        	 
			        if (response.status=='success') {    	 		     
			        jQuery('.fancybox-inner').css('height','auto');   	 
		        	jQuery('.contactus-form').hide();
		        	jQuery('.success-submitted').html(response.response);		        	
		        	jQuery('.success-submitted').show();		        	
			        } else {			        				        	
				        alert(response.response);
			        }			
			        jQuery('.btn-contactus').removeAttr('disabled'); 	         	
		        },
		        error: function(XMLHttpRequest, textStatus, errorThrown) {
                	alert( '<?php echo $this->__('There was issue with request. Please try again');?>' );
                	jQuery('.ajax-loader').hide();	                 	
                	jQuery('.btn-contactus').removeAttr('disabled'); 	
               	}
		    }); 	 		
 		}	
 	</script>
